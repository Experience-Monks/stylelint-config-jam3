{
  "plugins": [
    "dependencies"
  ],
  "rules": {
    "dependencies/min-vulnerabilities-allow": [
      "error",
      {
        "level": "high",
        "skip": ["1654", "1753", "1754"]
      }
    ],
    "package-json-properties": [
      "error",
      {
        "required": [
          "name",
          "license"
        ]
      }
    ],
    "root-files": [
      "error",
      {
        "required": [
          ".gitignore",
          "README.md",
          "LICENSE"
        ]
      }
    ]
  }
}
